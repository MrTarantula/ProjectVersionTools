{
  "manifestVersion": 1,
  "id": "projectversiontools",
  "version": "1.2.1",
  "name": "Project Version Tools",
  "scopes": [],
  "description": "Build tasks for reading and setting project versions.",
  "publisher": "mrtarantula",
  "public": true,
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [{
    "id": "Microsoft.VisualStudio.Services"
  }],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "ClickOnce",
    "Version",
    "Application Version",
    "Minimum Required Version",
    "vbproj",
    "csproj",
    "Variables"
  ],
  "categories": [
    "Build and release"
  ],
  "screenshots": [{
      "path": "static/images/setversion.png"
    },
    {
      "path": "static/images/variables.png"
    }
  ],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "readme.md"
    }
  },
  "links": {
    "support": {
      "uri": "https://github.com/MrTarantula/ProjectVersionTools/issues"
    },
    "repository": {
      "uri": "https://github.com/MrTarantula/ProjectVersionTools"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/MrTarantula/ProjectVersionTools"
  },
  "branding": {
    "color": "rgb(255, 255, 255)",
    "theme": "light"
  },
  "files": [{
      "path": "static/images",
      "addressable": true
    },
    {
      "path": "setprojectversion"
    },
    {
      "path": "versionintovariables"
    },
    {
      "path": "projectfileprops"
    }
  ],
  "contributions": [{
      "id": "setprojectversion",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "setprojectversion"
      }
    },
    {
      "id": "versionintovariables",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "versionintovariables"
      }
    },
    {
      "id": "projectfileprops",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "projectfileprops"
      }
    }
  ]
}